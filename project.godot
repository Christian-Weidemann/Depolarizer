; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Depolarizer"
run/main_scene="res://GameController/GameController.tscn"
config/features=PackedStringArray("4.0", "Forward Plus")
boot_splash/bg_color=Color(0.117647, 0.117647, 0.117647, 1)
boot_splash/image="res://Assets/logo.png"
boot_splash/fullsize=false
boot_splash/minimum_display_time=800

[autoload]

Global="*res://global.gd"

[display]

window/size/viewport_width=910
window/size/viewport_height=540
window/stretch/mode="viewport"

[global]

edge_width=10
node_radius=30
node_color_1=Color(0.827451, 0.192157, 0.192157, 1)
select_color=Color(1, 0.462745, 0.392157, 1)
node_color_2=Color(0.258824, 0.4, 0.760784, 1)

[physics]

2d/default_gravity=0.0
2d/default_gravity_vector=Vector2(0, 0)
2d/default_linear_damp=8.0
2d/default_angular_damp=8.0

[rendering]

anti_aliasing/quality/msaa_2d=1
anti_aliasing/quality/use_taa=true

[shader_globals]

node_radius={
"type": "int",
"value": 30
}
node_color={
"type": "color",
"value": Color(57.4, 100, 0, 1)
}
selected_node_color={
"type": "color",
"value": Color(1, 0.52549, 0.458824, 1)
}
edge_width={
"type": "int",
"value": 10
}
